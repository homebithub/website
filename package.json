    "@react-router/node": "^7.9.5",
    "@react-router/serve": "^7.9.5",
    "@tinymce/tinymce-react": "^5.1.1",
    "axios": "^1.7.7",
    "canvas-confetti": "^1.9.3",
    "chart.js": "^4.4.5",
    "chartjs-plugin-datalabels": "^2.2.0",
    "compression": "^1.8.1",
